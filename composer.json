{
    "name": "gh640/phpunit-tap",
    "description": "Provides a TAP result printer for PHPUnit.",
    "type": "library",
    "license": "MIT",
    "keywords": ["test","phpunit", "tap", "phpunit reporter"],
    "homepage": "https://github.com/gh640/phpunit-tap",
    "authors": [
        {
            "name": "Goto Hayato",
            "email": "habita.gh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "gh640\\PhpunitTap\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "symfony/yaml": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit tests",
        "test:success": "phpunit tests/SuccessTest",
        "test:failure": "phpunit tests/FailureTest"
    }
}
